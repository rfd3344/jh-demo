{
	"extends": "airbnb",
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"commonjs": true,
		"shared-node-browser": true,
		"mocha": true,
		"es6":true,
		"worker":true,
		"phantomjs":true
	},
	"plugins": [
		"chai-expect"
	],
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": 0,
		"prefer-arrow-callback": 0,
		"func-names": 0,
		"max-len": ["warn", {"code": 200}],
		"linebreak-style": 0,
		"import/no-unresolved": 0,
		"no-plusplus": 0,
		"no-restricted-globals": 0,
		"no-multiple-empty-lines": 0,
		"no-undef": 1
	}
}
